      "background_texture": "https://images.unsplash.com/photo-1577657071619-ba70d1b6f376?crop=entropy&cs=srgb&fm=jpg&ixid=M3w4NjAzMzl8MHwxfHNlYXJjaHwyfHxsdXh1cnklMjBtZXJjZWRlcyUyMGFtZyUyMGd0JTIwZGFyayUyMHN0dWRpbyUyMGxpZ2h0aW5nfGVufDB8fHx8MTc3MTUyOTEzOHww&ixlib=rb-4.1.0&q=85"
    },
    "competitions": {
      "mercedes_amg": "https://images.pexels.com/photos/26563767/pexels-photo-26563767.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940",
      "bmw_m4": "https://images.unsplash.com/photo-1706811422966-24bfa91695c3?crop=entropy&cs=srgb&fm=jpg&ixid=M3w3NDk1Nzh8MHwxfHNlYXJjaHw0fHxibXclMjBtNCUyMGNvbXBldGl0aW9uJTIwZGFyayUyMGFnZ3Jlc3NpdmUlMjBmcm9udCUyMGFuZ2xlfGVufDB8fHx8MTc3MTUyOTE0OXww&ixlib=rb-4.1.0&q=85",
      "audi_rs6": "https://images.unsplash.com/photo-1654855383391-765987e87c7f?crop=entropy&cs=srgb&fm=jpg&ixid=M3w4NjAzMjV8MHwxfHNlYXJjaHw0fHxhdWRpJTIwcnM2JTIwYXZhbnQlMjBncmV5JTIwZGFyayUyMGFlc3RoZXRpY3xlbnwwfHx8fDE3NzE1MjkxNTB8MA&ixlib=rb-4.1.0&q=85",
      "iphone_15": "https://images.pexels.com/photos/18525574/pexels-photo-18525574.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940",
      "ps5_setup": "https://images.pexels.com/photos/13189290/pexels-photo-13189290.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940"
    },
    "winners": {
      "happy_winner_1": "https://images.pexels.com/photos/6805162/pexels-photo-6805162.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940",
      "happy_winner_2": "https://images.pexels.com/photos/16966369/pexels-photo-16966369.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940"
    },
    "trust": {
      "payments": ["visa", "mastercard", "apple-pay", "google-pay"]
    }
  },
  "instructions_to_main_agent": [
    "Install 'lucide-react' and 'framer-motion' immediately.",
    "Update 'tailwind.config.js' with the colors defined above. Extend the theme, do not overwrite default Shadcn variables where possible, but override Primary and Background.",
    "Create a 'Navbar' that is sticky, glassmorphic, and uses the 'Unbounded' font for the logo.",
    "The Hero section must use the 'Main Car' image with a 'Gold Rush' gradient overlay for text readability.",
    "Competition Cards must have a 'Tickets Left' progress bar using the Secondary Color (Cyan).",
    "Use 'Sonner' for all notifications (e.g., 'Ticket Added to Cart').",
    "Ensure all forms (Login, Register) use the defined Input styles with the glowing focus state.",
    "The Admin Dashboard should be denser (Grid Mode B) but keep the dark theme aesthetic.",
    "Add 'data-testid' to all interactive elements."
  ],
  "universal_guidelines": [
    "Do not use generic 'Inter' for headings. Use 'Unbounded'.",
    "Avoid full-screen gradients. Use gradients only as subtle glows or text accents.",
    "Ensure 4.5:1 contrast ratio for all text.",
    "Mobile images max-height: 80vh.",
    "Use 2-3x padding for 'Luxury' feel."
  ]
}
